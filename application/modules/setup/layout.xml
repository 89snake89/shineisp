<?xml version="1.0" encoding="UTF-8"?>
<layout>

	<!-- Default configuration of the whole project -->
	<general template="2columns-left">
		<head>
		    <!-- JS files to be include in every pages-->
			<js override="1">/resources/js/jquery/jquery-1.6.2.min.js</js>  
			<js override="1">/resources/js/jquery/jquery-ui-1.8.16.custom.min.js</js>
			<js override="1">/resources/js/jquery/jquery.blockUI.js</js>
			<js override="1">/resources/js/jquery/jquery.cookie.js</js>
			
			<!-- CSS files to be include in every pages-->
			<css override="1">/resources/js/css/jquery-ui-1.8.16.custom.css</css>
		</head>
	</general>

    <!-- Setup Template Section -->
	<setup>
		<!-- Common files called at the start of the setup module and will be APPENDED to the page with the GENERAL head items -->
		<commons template="1column">
			<head>
				<css>/css/reset.css</css>
				<css>/css/base.css</css>
				<css>/css/style.css</css>
				<js>/js/setup.js</js>
			</head>
		</commons>

	</setup>

</layout>