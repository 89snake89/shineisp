<?xml version="1.0" encoding="UTF-8"?>
<layout>

	<!-- Default configuration of the whole project -->
	<general template="1column">
		<head>
		    <!-- JS files to be include in every pages-->
            <js override="1" position="admin_endbody">//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js</js>  
            <js override="1" position="admin_endbody">//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js</js>
            <js override="1" position="admin_endbody">/resources/jquery/jquery.cookie.js</js>
            
			<!--Bootstrap CSS framework conditional stylesheet for all the brower to be include in every frontend pages-->
            <css override="1">//netdna.bootstrapcdn.com/bootstrap/3.0.1/css/bootstrap.min.css</css>
            <js override="1" position="endbody">//netdna.bootstrapcdn.com/bootstrap/3.0.1/js/bootstrap.min.js</js>
		</head>
	</general>
	
    <!-- Admin Template Section -->
	<admin>
		<!-- Common files called at the start of the admin module and will be APPENDED to the page with the GENERAL head items -->
		<commons template="1column">
			<head>
			   
                <js override="1" position="admin_endbody">/resources/wysiwyg/wysihtml5/wysihtml-rules.js</js>
                <js override="1" position="admin_endbody">/resources/wysiwyg/wysihtml5/wysihtml5-0.3.0.min.js</js>
                <js override="1" position="admin_endbody">/resources/wysiwyg/wysihtml5/bootstrap-wysihtml5.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/bootstrap-typeahead.min.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/bootstrap-hogan-2.0.0.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/jquery.tokeninput.js</js>
                
                <js override="1" position="admin_endbody">/resources/jquery/jquery.ui.datepicker-it.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/jquery.dynatree.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/jquery.dataTables.min.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/jquery.dataTables.delay.js</js>
                
                <js override="1" position="admin_endbody">/resources/jquery/bootstrap-select.min.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/bootstrap-rating-input.min.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/bootstrap-switch.min.js</js>
                <js override="1" position="admin_endbody">/resources/jquery/bootstrap-filestyle.min.js</js>
                
                <js position="admin_endbody">/js/smartresize.js</js> 
                <js position="admin_endbody">/js/grid.js</js> 
                
                <css>/css/grid.css</css>  
                
                <!-- files to be include in every ADMIN pages-->
                <css override="1">/resources/wysiwyg/wysihtml5/bootstrap-wysihtml5.css</css>
                <css override="1">/resources/jquery/css/ui.dynatree.css</css>
                <css override="1">/resources/jquery/css/jquery.dataTables.css</css>
                <css override="1">/resources/jquery/css/bootstrap-select.min.css</css>
                <css override="1">/resources/jquery/css/bootstrap-switch.css</css>
                <css override="1">/resources/jquery/css/bootstrap-typeahead.css</css>
			</head>
		</commons>
		 
		<index>
			<head>
				<js position="admin_endbody">/js/login.js</js> 
				<js position="admin_endbody">/js/widget.js</js> 
				<css>/css/widget.css</css> 
			</head>
			<!--
			<blocks>
			     <block side="endcontent">contacts</block>
			</blocks>
			-->
		</index>
	</admin>

</layout>